{
  "manifest_version": 2,

  "name": "Js QR Code generator",
  "description": "This extension to generated qr code by pure javascript.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "js-qrcode.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs","contextMenus"
  ],
  "background": {
    "scripts": ["createmenu.js"]
  }

}
